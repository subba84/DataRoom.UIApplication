@model List<DataRooms.UI.Models.File>


<table class="table table-bordered" cellpadding="0" cellspacing="0">
    <thead class="tbl-header">
        <tr>
            <th>SharBox</th>
            <th>File/Item Tracker Name</th>
            <th>Uploaded By</th>
            <th>Uploaded On</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {
            foreach (var file in Model)
            {
                <tr>
                    <td>@file.DataRoomName</td>
                    <td>@file.FileName</td>
                    <td>@file.CreatorName</td>
                    <td>@file.CreatedOn.ToApplicationFormat()</td>
                </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="4">No Recent Uploads</td>
            </tr>
        }
    </tbody>
</table>



